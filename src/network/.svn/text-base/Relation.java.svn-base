package network;

import java.io.*;

public class Relation implements Serializable {

	private static final long serialVersionUID = -7336382566162733140L;

	// Auto generated id
	long id = -1;

	// Model parameter
	double ltWeight = 0.0;
	double icProbability = 0.0;
	double htWeight = 0.0;
	double sisBirthRate = 0.0;
	double sirBirthRate = 0.0;
	double weight = 0.0;
	double legnth = 0.0;
	double href = 0.0;
	double img = 0.0;
	double score = 0.0;
	boolean predicted = false;
	
	public Relation(long id) {
		super();
		this.id = id;
	}

	/**
	 * @return the id
	 */
	public long getId() {
		return id;
	}

	/**
	 * @return the ltWeight
	 */
	public double getLtWeight() {
		return ltWeight;
	}

	/**
	 * @param ltWeight the ltWeight to set
	 */
	public void setLtWeight(double ltWeight) {
		this.ltWeight = ltWeight;
	}

	/**
	 * @return the icProbability
	 */
	public double getIcProbability() {
		return icProbability;
	}

	/**
	 * @param icProbability the icProbability to set
	 */
	public void setIcProbability(double icProbability) {
		this.icProbability = icProbability;
	}

	/**
	 * @return the htWeight
	 */
	public double getHtWeight() {
		return htWeight;
	}

	/**
	 * @param htWeight the htWeight to set
	 */
	public void setHtWeight(double htWeight) {
		this.htWeight = htWeight;
	}

	/**
	 * @return the sisBirthRate
	 */
	public double getSisBirthRate() {
		return sisBirthRate;
	}

	/**
	 * @param sisBirthRate the sisBirthRate to set
	 */
	public void setSisBirthRate(double sisBirthRate) {
		this.sisBirthRate = sisBirthRate;
	}

	/**
	 * @return the sirBirthRate
	 */
	public double getSirBirthRate() {
		return sirBirthRate;
	}

	/**
	 * @param sirBirthRate the sirBirthRate to set
	 */
	public void setSirBirthRate(double sirBirthRate) {
		this.sirBirthRate = sirBirthRate;
	}
	
	/**
	 * @return String format of all Relation information
	 * @see java.lang.String#toString()
	 */
	public String toString() {
		return Long.toString(id);
	}

	/**
	 * @return the weight
	 */
	public double getWeight() {
		return weight;
	}

	/**
	 * @param weight the weight to set
	 */
	public void setWeight(double weight) {
		this.weight = weight;
	}

	/**
	 * @return the legnth
	 */
	public double getLegnth() {
		return legnth;
	}

	/**
	 * @param legnth the legnth to set
	 */
	public void setLegnth(double legnth) {
		this.legnth = legnth;
	}

	/**
	 * @return the href
	 */
	public double getHref() {
		return href;
	}

	/**
	 * @param href the href to set
	 */
	public void setHref(double href) {
		this.href = href;
	}

	/**
	 * @return the img
	 */
	public double getImg() {
		return img;
	}

	/**
	 * @param img the img to set
	 */
	public void setImg(double img) {
		this.img = img;
	}

	/**
	 * @return the predicted
	 */
	public boolean isPredicted() {
		return predicted;
	}

	/**
	 * @param predicted the predicted to set
	 */
	public void setPredicted(boolean predicted) {
		this.predicted = predicted;
	}

	/**
	 * @return the score
	 */
	public double getScore() {
		return score;
	}

	/**
	 * @param score the score to set
	 */
	public void setScore(double score) {
		this.score = score;
	}
}
